                          skos:Concept ;
                 skos:inScheme pachl: ;
                 skos:narrower pachl:Concept857 ,
                               pachl:Concept931 ;
                 fidvocs:dateOfSource "2019-06-30"^^xsd:date ;
                 rdfs:label "conflicting routes"@en ;
