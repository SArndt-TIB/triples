                 fidvocs:dateOfSource "2019-06-30"^^xsd:date ;
                 rdfs:label "caboose"@en ;
                 skos:definition "A staffed car at the rear end of freight trains on US railways. Today, a caboose is only required on lines with time spacing."@en .


###  https://www.fid-move.de/vocabulary/pachl#Concept682
pachl:Concept682 rdf:type owl:NamedIndividual ,
                          skos:Concept ;
                 skos:inScheme pachl: ;
                 fidvocs:dateOfSource "2019-06-30"^^xsd:date ;
